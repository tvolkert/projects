#!/bin/bash

. $PROJ_BASE/etc/defs.sh || exit $?
[[ $(basename $PWD) == "ios" ]] || die "$(basename $0) must be run from ios dir"

set -ex

env xcrun xcodebuild clean build \
      -configuration Debug \
      ONLY_ACTIVE_ARCH=YES \
      -sdk iphoneos \
      -arch arm64
      # -sdk iphonesimulator \
      # -arch x86_64
